{
  "name": "react-file-showable-chat-ui",
  "version": "1.0.5",
  "description": "A library of React components for building chat UI's",
  "repository": "https://github.com/varmil/react-file-showable-chat-ui",
  "main": "./lib/index.js",
  "scripts": {
    "build-demo": "npx babel --presets es2015,stage-0 ./demo/index.js -o ./demo/es5.js && npx browserify demo/es5.js -o demo/bundle.js",
    "demo": "npx live-server demo/",
    "compile": "./precompile.sh",
    "prepublish": "yarn compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watch \"npm run compile && npm run build-demo\" ./src"
  },
  "keywords": [
    "react",
    "chat",
    "chatfeed",
    "reactjs",
    "ui",
    "messenger"
  ],
  "author": "Akihiro Yamamoto",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-linkify": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "browserify": "^16.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-linkify": "^1.0.0-alpha",
    "typescript": "^3.7.5",
    "watch": "^1.0.2"
  }
}
